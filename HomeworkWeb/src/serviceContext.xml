	<bean id="coursewareService" class="me.hupeng.homeworkweb.service.CoursewareService" scope="prototype" autowire="default"></bean>
	<bean id="classnameService" class="me.hupeng.homeworkweb.service.ClassnameService" scope="prototype" autowire="default"></bean>
