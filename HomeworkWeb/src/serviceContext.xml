xsi:schemaLocation="
http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context.xsd
http://servicemix.apache.org/http/1.0
http://servicemix.apache.org/schema/servicemix-http-3.2.3.xsd"
default-autowire="byName">
	<bean id="userService" class="me.hupeng.homeworkweb.service.UserService" scope="prototype" autowire="default"></bean>
	<bean id="classesService" class="me.hupeng.homeworkweb.service.ClassesService" scope="prototype" autowire="default"></bean>
